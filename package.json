{
	"name": "ko-en-conversion",
	"displayName": "%displayName%",
	"description": "한영 변환 도구 Korean-English conversion tool",
	"version": "0.5.0",
	"publisher": "LeeSeungYun1020",
	"repository": {
		"type": "git",
		"url": "https://github.com/LeeSeungYun1020/vscode-ko-en-conversion"
	},
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"set"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ko-en-conversion.conversion",
				"title": "%command.conversion.title%"
			},
			{
				"command": "ko-en-conversion.korean",
				"title": "%command.korean.title%",
				"shortTitle": "%command.korean.title.short%"
			},
			{
				"command": "ko-en-conversion.english",
				"title": "%command.english.title%",
				"shortTitle": "%command.english.title.short%"
			}
		],
		"menus": {
			"editor/context": [
				{
					"group": "1_modification",
					"command": "ko-en-conversion.conversion",
					"when": "config.ko-en-conversion.command.menu.display"
				},
				{
					"group": "1_modification",
					"command": "ko-en-conversion.korean",
					"when": "config.ko-en-conversion.command.menu.display && config.ko-en-conversion.command.menu.korean"
				},
				{
					"group": "1_modification",
					"command": "ko-en-conversion.english",
					"when": "config.ko-en-conversion.command.menu.display && config.ko-en-conversion.command.menu.english"
				}
			]
		},
		"configuration": {
			"title": "%configuration.title%",
			"properties": {
				"ko-en-conversion.command.menu.display": {
					"description": "%configuration.command.menu.display%",
					"type": "boolean",
					"default": true
				},
				"ko-en-conversion.command.menu.korean": {
					"description": "",
					"type": "boolean",
					"default": false
				},
				"ko-en-conversion.command.menu.english": {
					"description": "",
					"type": "boolean",
					"default": false
				},
				"ko-en-conversion.command.action.display": {
					"description": "",
					"type": "boolean",
					"default": true
				},
				"ko-en-conversion.command.action.preview": {
					"description": "",
					"type": "boolean",
					"default": true
				},
				"ko-en-conversion.command.action.korean": {
					"description": "",
					"type": "boolean",
					"default": false
				},
				"ko-en-conversion.command.action.english": {
					"description": "",
					"type": "boolean",
					"default": false
				},
				"ko-en-conversion.language.target": {
					"markdownDescription": "",
					"type": "string",
					"default": "",
					"enum": [
						"",
						"",
						""
					],
					"enumDescription": [
						"",
						"",
						""
					]
				}
			}
		},
		"keybindings": [
			{
				"command": "ko-en-conversion.conversion",
				"key": "ctrl+alt+c"
			},
			{
				"command": "ko-en-conversion.korean",
				"key": "ctrl+alt+g"
			},
			{
				"command": "ko-en-conversion.english",
				"key": "ctrl+alt+e"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"clean": "gulp clean"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"del": "^6.0.0",
		"event-stream": "^4.0.1",
		"gulp": "^4.0.2",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-typescript": "^6.0.0-alpha.1",
		"run-sequence": "^2.2.1",
		"vsce": "^2.6.7",
		"vscode-nls": "^5.0.0",
		"vscode-nls-dev": "^4.0.0"
	}
}
